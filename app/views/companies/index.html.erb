<h1>All the Companies</h1>
<%= link_to "New Game", new_company_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Start_date</th>
    <th>Country</th>
  </tr>
  <% @companies.each do |company| %>
    <tr>
      <td><%= company.name %></td>
      <td><%= company.description  %></td>
      <td><%= company.country  %></td>
      <td><%= link_to "Show", "/companies/#{company.id}"  %></td>
    </tr>
  <% end %>
</tbody>
</table>
<%= link_to "Create a new game", new_company_path%>
